 	WARN(1, "Invalid mem subsystem");
 	return NULL;
 }
diff --git a/drivers/media/pci/intel/ipu-wrapper.h b/drivers/media/pci/intel/ipu-wrapper.h
index b7df285..52ca2d1 100644
