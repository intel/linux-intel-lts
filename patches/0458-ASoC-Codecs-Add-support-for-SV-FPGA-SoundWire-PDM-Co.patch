+	wr_msg.slave_addr = slave->slv_number;
+	wr_msg.addr_page1 = 0x0;
+	wr_msg.addr_page2 = 0x0;
+	wbuf[0] = 0x0;
+	ret = sdw_slave_transfer(slave->mstr, &wr_msg, 1);
+	if (ret != 1) {
+		ret = -EINVAL;
+		dev_err(&slave->dev, "Interrupt status read failed for slave %x\n", slave->slv_number);
+		goto out;
+	}
+	wr_msg.addr = 0x1804;
+	wbuf[0] = 0x1;
+	ret = sdw_slave_transfer(slave->mstr, &wr_msg, 1);
+	if (ret != 1) {
+		ret = -EINVAL;
+		dev_err(&slave->dev, "Interrupt status read failed for slave %x\n", slave->slv_number);
+		goto out;
+	}
+	rd_msg.ssp_tag = 0;
+	rd_msg.flag = SDW_MSG_FLAG_READ;
+	rd_msg.addr = 0x180b;
+	rd_msg.len = 1;
+	rd_msg.buf = rbuf;
+	rd_msg.slave_addr = slave->slv_number;
+	rd_msg.addr_page1 = 0x0;
+	rd_msg.addr_page2 = 0x0;
+	ret = sdw_slave_transfer(slave->mstr, &rd_msg, 1);
+	if (ret != 1) {
+		ret = -EINVAL;
+		dev_err(&slave->dev, "Interrupt status read failed for slave %x\n", slave->slv_number);
+		goto out;
+	}
+	wr_msg.addr = 0x180b;
+	wbuf[0] = 0x1a;
+	ret = sdw_slave_transfer(slave->mstr, &wr_msg, 1);
+	if (ret != 1) {
+		ret = -EINVAL;
+		dev_err(&slave->dev, "Interrupt status read failed for slave %x\n", slave->slv_number);
+		goto out;
+	}
+	wr_msg.addr = 0x180c;
+	wbuf[0] = 0x0;
+	ret = sdw_slave_transfer(slave->mstr, &wr_msg, 1);
+	if (ret != 1) {
+		ret = -EINVAL;
+		dev_err(&slave->dev, "Interrupt status read failed for slave %x\n", slave->slv_number);
+		goto out;
+	}
+	wr_msg.addr = 0x180d;
+	wbuf[0] = 0x0;
+	ret = sdw_slave_transfer(slave->mstr, &wr_msg, 1);
+	if (ret != 1) {
+		ret = -EINVAL;
+		dev_err(&slave->dev, "Interrupt status read failed for slave %x\n", slave->slv_number);
+		goto out;
+	}
+	wr_msg.addr = 0x180e;
+	wbuf[0] = 0x0;
+	ret = sdw_slave_transfer(slave->mstr, &wr_msg, 1);
+	if (ret != 1) {
+		ret = -EINVAL;
+		dev_err(&slave->dev, "Interrupt status read failed for slave %x\n", slave->slv_number);
+		goto out;
+	}
+	wr_msg.addr = 0x180f;
+	wbuf[0] = 0x0;
+	ret = sdw_slave_transfer(slave->mstr, &wr_msg, 1);
+	if (ret != 1) {
+		ret = -EINVAL;
+		dev_err(&slave->dev, "Interrupt status read failed for slave %x\n", slave->slv_number);
+		goto out;
+	}
+	wr_msg.addr = 0x1810;
+	wbuf[0] = 0xb8;
+	ret = sdw_slave_transfer(slave->mstr, &wr_msg, 1);
+	if (ret != 1) {
+		ret = -EINVAL;
