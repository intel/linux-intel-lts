+	va->sd.do_unregister = ti964_unregistered;
+	i2c_set_clientdata(client, &va->sd);
+	for (i = 0; i < NR_OF_TI964_SINK_PADS; i++) {
+		va->ffmts[i]->ffmt.width = 1280;
+		va->ffmts[i]->ffmt.height = 720;
+		va->ffmts[i]->ffmt.pixelformat = ICI_FORMAT_UYVY;
