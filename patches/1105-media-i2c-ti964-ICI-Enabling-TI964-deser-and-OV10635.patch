+	struct ici_ext_subdev *subdev = i2c_get_clientdata(client);;
+
+	if (!subdev)
+		return -EINVAL;
+
+	va = to_ti964(subdev);
+
+	if (!va)
+		return -EINVAL;
+
+	return ti964_init(va);
+}
+#else
+#define ti964_suspend	NULL
+#define ti964_resume	NULL
