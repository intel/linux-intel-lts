+static int __crlmodule_rect_index(enum crl_subdev_type type,
+				  const struct crl_mode_rep *mode)
+{
+	int i;
+
+	for (i = 0; i < mode->sd_rects_items; i++) {
+		if (type == mode->sd_rects[i].subdev_type)
+			return i;
+	}
+
+	return -1;
+}
